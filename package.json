{
  "name": "seityan",
  "version": "1.1.2",
  "description": "Seityan - a desktop assistant",
  "main": "dist/main.js",
  "scripts": {
    "start": "cd dist && electron .",
    "watch": "webpack --watch",
    "build": "webpack",
    "test": "mocha -r ts-node/register test/main-test.ts test/test.ts",
    "test-api": "mocha -r ts-node/register test/apitest.ts",
    "package": "npm run package-win32-x64 && npm run package-linux-x64 && npm run package-osx-x64",
    "package-win32-x64": "electron-packager ./dist seityan --overwrite --asar=true --platform=win32 --arch=x64 --out=release-builds --icon=dist/assets/image/win.ico",
    "package-linux-x64": "electron-packager ./dist seityan --overwrite --asar=true --platform=linux --arch=x64 --out=release-builds",
    "package-osx-x64": "electron-packager ./dist seityan --overwrite --asar=true --platform=darwin --arch=x64 --out=release-builds --icon=dist/assets/image/mac.icns",
    "dialog-test": "electron test/dialog-lancher.js",
    "weather-test": "electron test/weather-lancher.js",
    "build-doc": "typedoc --out ./doc/html --exclude '**/utils/conf.ts' --excludeExternals ./app"
  },
  "author": "",
  "license": "MIT",
  "repository": "https://github.com/Norgerman/Seityan",
  "devDependencies": {
    "@types/chai": "^ 4.1.2",
    "@types/electron": "1.4.35",
    "@types/material-ui": "^0.20.5",
    "@types/mocha": "^2.2.48",
    "@types/pixi.js": "^4.7.0",
    "@types/react": "^16.0.36",
    "@types/react-dom": "^16.0.3",
    "@types/react-tap-event-plugin": "0.0.30",
    "@types/rimraf": "2.0.2",
    "@types/webpack-env": "^1.13.5",
    "chai": "^4.1.2",
    "electron": "1.4.13",
    "electron-packager": "^10.1.2",
    "mocha": "^4.0.1",
    "ts-loader": "^3.1.1",
    "ts-node": "^4.1.0",
    "typedoc": "^0.10.0",
    "typescript": "^2.7.1",
    "webpack": "^3.10.0"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "decompress-zip": "git://github.com/sunxfancy/decompress-zip",
    "electron-dl": "^1.10.0",
    "material-ui": "^0.20.0",
    "pixi-live2d": "git://github.com/sunxfancy/pixi-live2d",
    "pixi.js": "^4.7.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-tap-event-plugin": "^3.0.2",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "tone": "^0.11.12",
    "uuid": "^3.2.1"
  }
}
